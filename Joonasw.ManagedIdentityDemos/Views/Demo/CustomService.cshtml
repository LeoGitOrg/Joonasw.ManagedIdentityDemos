@model CustomServiceViewModel
@{
    ViewData["Title"] = "Custom API";
}

<h2>Custom API</h2>

<p>Called API, received user claims that it extracted from the access token.</p>

<table class="table table-striped">
    <thead>
        <tr>
            <th>Claim type</th>
            <th>Value</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var claim in Model.Claims)
        {
            <tr>
                <td>@claim.Key</td>
                <td>@claim.Value</td>
            </tr>
        }
    </tbody>
</table>